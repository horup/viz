{
  "name": "viz",
  "version": "1.0.0",
  "description": "Starter template with typescript, index.html, react and webpack support out of the box",
  "scripts": {
    "build": "webpack",
    "watch": "concurrently --kill-others \"webpack --watch\" \"ws\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/horup/typescript-webpack-react-template.git"
  },
  "author": "Søren Alsbjerg Hørup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/horup/typescript-webpack-react-template/issues"
  },
  "homepage": "https://github.com/horup/typescript-webpack-react-template#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "concurrently": "^3.5.0",
    "local-web-server": "^1.2.8",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0"
  },
  "local-web-server": {
    "port": 8080,
    "rewrite": [
      {
        "from": "/*",
        "to": "/dist/$1"
      }
    ]
  },
  "dependencies": {
    "@types/react": "^15.0.32",
    "@types/react-dom": "^15.5.1",
    "@types/vis": "^4.18.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "vis": "^4.20.0"
  },
  "main": "webpack.config.js"
}
